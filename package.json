{
  "name": "@omnicar/sam-format",
  "description": "Library for formatting different kinds of data according to type and locale",
  "author": "Omnicar A/S",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@omnicar/sam-types": "0.0.275",
    "@types/date-fns": "2.6.0",
    "@types/jest": "26.0.23",
    "@types/node": "^11.13.0",
    "jest": "^24.7.0",
    "ts-jest": "^24.0.1",
    "typescript": "^3.4.1"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watchAll --collectCoverage",
    "tsc": "npx tsc -p ./tsconfig.json -w",
    "tsc-once": "npx tsc -p tsconfig.json && npx tsc -p tsconfig.locales.json"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "automock": false,
    "setupFiles": [],
    "testPathIgnorePatterns": [
      "./build"
    ]
  },
  "dependencies": {
    "date-fns": "v2.10.0",
    "format-number": "^3.0.0",
    "tslib": "^1.9.3"
  },
  "version": "0.0.36"
}
