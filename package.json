{
  "name": "@omnicar/sam-format",
  "description": "Library for formatting different kinds of data according to type and locale",
  "author": "Omnicar A/S",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.0.0",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watchAll --collectCoverage",
    "tsc": "node node_modules/typescript/bin/tsc -p ./tsconfig.json -w",
    "tsc-once": "node node_modules/typescript/bin/tsc -p ./tsconfig.json"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "automock": false,
    "setupFiles": [],
    "testPathIgnorePatterns": [
      "./build"
    ]
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "format-number": "^3.0.0",
    "@omnicar/sam-types": "0.0.71",
    "tslib": "^1.9.0"
  }
}
